@article{cespedesEvaluatingLinguisticCoverage2025,
  title = {Evaluating the Linguistic Coverage of {{{\textsc{OpenAlex}}}} : {{An}} Assessment of Metadata Accuracy and Completeness},
  shorttitle = {Evaluating the Linguistic Coverage Of},
  author = {C√©spedes, Luc√≠a and Kozlowski, Diego and Pradier, Carolina and Sainte‚ÄêMarie, Maxime Holmberg and Shokida, Natsumi Solange and Benz, Pierre and Poitras, Constance and Ninkov, Anton Boudreau and Ebrahimy, Saeideh and Ayeni, Philips and Filali, Sarra and Li, Bing and Larivi√®re, Vincent},
