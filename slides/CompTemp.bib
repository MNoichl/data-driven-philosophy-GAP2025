@article{ortegaIndexationRetractedLiterature2024,
  title = {The Indexation of Retracted Literature in Seven Principal Scholarly Databases: A Coverage Comparison of Dimensions, {{OpenAlex}}, {{PubMed}}, {{Scilit}}, {{Scopus}}, {{The Lens}} and {{Web}} of {{Science}}},
  shorttitle = {The Indexation of Retracted Literature in Seven Principal Scholarly Databases},
  author = {Ortega, Jos√© Luis and Delgado-Quir√≥s, Lorena},
