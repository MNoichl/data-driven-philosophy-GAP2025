  issn = {1879-4920},
  doi = {10.1007/s13194-016-0145-1},
  url = {https://doi.org/10.1007/s13194-016-0145-1},
  urldate = {2025-03-25},
  abstract = {One striking feature of the contemporary modelling practice is its interdisciplinary nature. The same equation forms, and mathematical and computational methods, are used across different disciplines, as well as within the same discipline. Are there, then, differences between intra- and interdisciplinary transfer, and can the comparison between the two provide more insight on the challenges of interdisciplinary theoretical work? We will study the development and various uses of the Ising model within physics, contrasting them to its applications to socio-economic systems. While the renormalization group (RG) methods justify the transfer of the Ising model within physics ‚Äì by ascribing them to the same universality class ‚Äì its application to socio-economic phenomena has no such theoretical grounding. As a result, the insights gained by modelling socio-economic phenomena by the Ising model may remain limited.},
  langid = {english},
  keywords = {Interdisciplinarity,Ising model,Physics,Renormalization group theory,Socio-economic systems}
}

@article{knuuttilaModelTemplatesTransdisciplinary2023,
  title = {Model Templates: Transdisciplinary Application and Entanglement},
